package exercicio;

public class TesteProduto {
	
	public static void main(String[] args) {
		Produto p1 = new Produto(2, 10.0, "feijao");
		
		System.out.println("Produto: "+ p1.getNome());
		System.out.println("Valor Unit√°rio: "+ p1.getValor());
		System.out.println("Quantidade: " + p1.getQuantidade());
		System.out.println("Valor: " + p1.getTotal());
		System.out.println("Total com ICMS: " + p1.calcIcms());
		
		
	}
}
