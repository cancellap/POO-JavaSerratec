package aula03;

public class GerarFolha {

	public static void main(String[] args) {

		Funcionario funcionario = new Funcionario();
		funcionario.nome = "Pedro";
		funcionario.cpf = "123";
		funcionario.salario = 1400.;
		
		if(funcionario.salario > 1412.) {
			System.out.println(funcionario.cacularFolha());
		}
	}

}
